{
  "name": "fml-fmlpp-vscode",
  "displayName": "FML / FML++ Syntax Support",
  "description": "Full VS Code support for FML and FML++ with syntax highlighting and XML conversion.",
  "version": "1.0.0",
  "publisher": "floppyiphone99",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/floppyiphone99/fml-fmlpp-vscode.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "fml",
        "aliases": ["FML", "fmlsucks"],
        "extensions": [".fml", ".fmlsucks"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "fmlpp",
        "aliases": ["FML++", "fmlpp"],
        "extensions": [".fmlpp", ".fml++"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fml",
        "scopeName": "source.fml",
        "path": "./syntaxes/fml.tmLanguage.json"
      },
      {
        "language": "fmlpp",
        "scopeName": "source.fmlpp",
        "path": "./syntaxes/fmlpp.tmLanguage.json"
      }
    ]
  }
}
